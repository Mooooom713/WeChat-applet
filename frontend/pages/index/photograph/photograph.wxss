.choiceContainer {
    display: flex;
    height: 80rpx;
    flex-wrap: wrap;
    background-color: #fff;
    border-bottom: 1rpx solid #eee;
    font-size: 28rpx;
}

.btn {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 375rpx;
    height: 80rpx;
    background-color: #fff;
    border-right: 1rpx solid #eee;
    border-top: 1rpx solid #eee;
    box-sizing: border-box;
}