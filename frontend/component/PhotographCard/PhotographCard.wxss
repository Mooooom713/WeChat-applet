.container {
    width: 100%;
    margin-top: 10px;
    border-bottom: 1px solid #dddddd;
    padding-bottom: 10px;
}

.content-wrap {
    width: 100%;
    margin-top: 10px;
}

.swiper-image {
    width: 100%;
    height: 100%;
}

.text-wrap,
.number-wrap {
    margin: 0 10px;
}

.button-wrap {
    width: 100%;
    height: 2rem;
    display: flex;
    flex-direction: row;
    margin-top: 10px;
}

.thumbs-up-btn {
    height: 100%;
    flex: 0 0 40%;
    margin: 0 5%;
    background: #ffffff url("data:image/svg+xml;base64,") no-repeat;
    background-size: 40% 80%;
    background-position: center right;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.report-btn {
    height: 100%;
    flex: 0 0 30%;
    margin: 0 5%;
    background: #ffffff url("data:image/svg+xml;base64,") no-repeat;
    background-size: 40% 80%;
    background-position: center right;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.active {
    color: #ff9700;
}

.number-wrap {
    margin-top: 10px;
    color: #ff9700;
}